<div class="edit-form">
<h5>Edit Profile</h5>
<%= form_for @user do |f| %>
  <strong>First Name:</strong> <%= f.text_field :first_name %><br>
  <Last Name: <%= f.text_field :last_name %><br>
  Email: <%= f.text_field :email %><br>
  User Name: <%= f.text_field :user_name %><br>
  Profile Picture: <%= f.text_field :image_url %><br>
  Course Taken at GA:<%= f.collection_select :course_taken, User::COURSE, :to_s, :to_s, :include_blank => false, :prompt => "Select Option" %> <br>
  Campus: <%= f.collection_select :city, User::CITIES, :to_s, :to_s, :include_blank => false, :prompt => "Select Option" %> <br />
  <%= f.submit "Submit" %>
<% end %>
</div>
